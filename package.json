{
  "name": "vscode-gem-lens",
  "displayName": "Gem Lens",
  "icon": "images/logo.png",
  "description": "Shows the latest version of a gem in Gemfile and gemspec",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-gem-lens.git"
  },
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "license": "MIT",
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.37.0"
  },
  "keywords": [
    "gem"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.gemspec",
    "workspaceContains:Gemfile"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src --ext .ts",
    "fmt": "prettier --write src/*.ts src/**/*.ts"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/jest": "26.0.12",
    "@types/moxios": "0.4.9",
    "@types/node": "14.0.27",
    "@types/vscode": "1.47.0",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "coveralls": "3.1.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "glob": "7.1.6",
    "jest": "26.3.0",
    "moxios": "0.4.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.4",
    "tslint": "6.1.3",
    "typescript": "3.9.7",
    "vscode-test": "1.4.0"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
