{
  "name": "vscode-gem-lens",
  "displayName": "Gem Lens",
  "icon": "images/logo.png",
  "description": "Shows the latest version of a gem in Gemfile and gemspec",
  "repository": {
    "type": "git",
    "url": "https://github.com/ninoseki/vscode-gem-lens.git"
  },
  "author": "Manabu Niseki",
  "publisher": "ninoseki",
  "license": "MIT",
  "version": "0.3.2",
  "engines": {
    "vscode": "^1.69.0"
  },
  "keywords": [
    "gem"
  ],
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.gemspec",
    "workspaceContains:Gemfile"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext ts",
    "dev": "npm run build --watch",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info",
    "vscode:prepublish": "npm run build",
    "publish": "vsce publish",
    "pack": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.3",
    "@types/vscode": "1.69.0",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "coveralls": "3.1.1",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "glob": "8.0.3",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "tslint": "6.1.3",
    "tsup": "^6.2.1",
    "typescript": "4.7.4",
    "vscode-test": "1.6.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.7"
  }
}
